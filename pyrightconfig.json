{
    "include": [
        "."
    ],
    "exclude": [
        "**/node_modules",
        "**/__pycache__",
        ".git"
    ],
    "ignore": [
        "**/venv",
        "**/env"
    ],
    "reportMissingImports": "warning",
    "reportMissingTypeStubs": false,
    "pythonVersion": "3.8",
    "pythonPlatform": "Windows",
    "executionEnvironments": [
        {
            "root": ".",
            "pythonVersion": "3.8",
            "pythonPlatform": "Windows",
            "extraPaths": [
                "./custom_nodes"
            ]
        }
    ],
    "typeCheckingMode": "basic",
    "useLibraryCodeForTypes": true,
    "autoImportCompletions": true,
    "stubPath": "./typings",
    "venvPath": ".",
    "venv": "venv"
} 